<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.zuehlke.zfb.control.ContentControl">
    <top>
        <VBox>       
            <fx:include source="menu.fxml"/>
            <fx:include source="navigationBar.fxml"/>
        </VBox>
    </top>
    <center>
        <SplitPane dividerPositions="0.3" focusTraversable="true" fx:id="content">
            <items>
                <fx:include source="tree.fxml"/>
                <Group></Group>
            </items>
        </SplitPane>
    </center>
    <bottom>
        <VBox>
            <fx:include source="StatusBar.fxml" />
        </VBox>
    </bottom>
</BorderPane>
